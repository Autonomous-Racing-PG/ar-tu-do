<?xml version="1.0"?>
<launch>
<!-- 
Launches all necessary nodes that listen for drive_parameters and translate them to the car
-->

    <!-- car control node -->
    <node
      respawn="true"
      pkg="car_control"
      type="car_controller"
      name="car_controller"
      output="screen" >
    </node>

    <!-- dms control node -->
    <node
      respawn="true"
      pkg="car_control"
      type="dms_controller"
      name="dms_controller"
      output="screen" >
    </node>
	
</launch>