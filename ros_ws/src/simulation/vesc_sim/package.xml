<?xml version="1.0" encoding="UTF-8"?>
<package format="2">
  <name>vesc_sim</name>
  <version>0.0.0</version>
  <description>The "vesc_sim" package emulates the vesc controller (focbox) and relays the drive commands to the simulated Gazebo car.</description>

  <!-- Maintainer -->
  <maintainer email="f110@ls12.cs.uni-dortmund.de">PG F1/10</maintainer>

  <!-- License -->
  <license>MIT</license>
  <license>GPLv3</license>

  <!-- Dependencies -->
  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>yaml-cpp</build_depend>

  <depend>drive_msgs</depend>
  <depend>std_msgs</depend>
  <depend>roscpp</depend>
  <depend>roslib</depend>
  <depend>rospy</depend>
  <depend>nav_msgs</depend>
  <depend>vesc_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>tf</depend>
</package>
